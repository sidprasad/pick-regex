{
  "name": "pick",
  "displayName": "pick",
  "description": "Generate a REGEX from LLMs and human classifications.",
  "publisher": "REPLACE_WITH_YOUR_PUBLISHER_ID",
  "version": "0.0.1",
  "repository": "https://github.com/sidprasad/pick-regex",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pick-explorer",
          "title": "PICK Regex Learner",
          "icon": "$(regex)"
        }
      ]
    },
    "views": {
      "pick-explorer": [
        {
          "type": "webview",
          "id": "pick.pickView",
          "name": "Regex Learner",
          "icon": "$(regex)"
        }
      ]
    },
    "commands": [],
    "configuration": {
      "title": "PICK Regex Learner",
      "properties": {
        "pick.eliminationThreshold": {
          "type": "number",
          "default": 2,
          "minimum": 1,
          "description": "Number of negative votes required to eliminate a candidate regex"
        },
        "pick.llm.vendor": {
          "type": "string",
          "default": "copilot",
          "enum": ["copilot", "openai", "anthropic"],
          "description": "LLM vendor to use for generating candidate regexes"
        },
        "pick.llm.family": {
          "type": "string",
          "default": "gpt-4o",
          "description": "LLM model family to use (e.g., gpt-4o, claude-3-5-sonnet, o1-preview)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package:vsix": "npx vsce package",
    "publish:vsce": "npx vsce publish -p $VSCE_TOKEN",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vsce": "^2.9.0"
  },
  "dependencies": {
    "randexp": "^0.5.3",
    "@gruhn/regex-utils": "^2.7.3"
  }
}
